    </Put>
    <Put override="true">
        <Key>
            <Parameter>certificate2_kid</Parameter>
        </Key>
        <Value ref="private.certificate1_kid"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>certificate1</Parameter>
        </Key>
        <Value ref="certificate1"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>certificate1_kid</Parameter>
        </Key>
        <Value ref="certificate1_kid"/>
    </Put>
