<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="ReturnVersion">
    <DisplayName>ReturnVersion</DisplayName>
    <Properties/>    
    <AssignVariable>
        <Name>response.status.code</Name>
        <Value>200</Value>       
    </AssignVariable>
        <AssignVariable>
        <Name>response.content</Name>
        <Value>1.1.0</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>response.header.Content-Type</Name>
        <Value>text/plain</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
